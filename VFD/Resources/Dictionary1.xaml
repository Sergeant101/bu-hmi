                    <Path.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform ScaleX="-1"/>
                            <SkewTransform/>
                            <RotateTransform Angle="90"/>
                        </TransformGroup>
                    </Path.RenderTransform>
                </Path>
            </Canvas>
            <Path SnapsToDevicePixels="True" StrokeThickness="2.0" Stroke="#ff000000" StrokeMiterLimit="1.0" Data="F1 M 0,0 L 10,0" Stretch="Fill" Canvas.Left="26" Canvas.Top="18" RenderTransformOrigin="0.5,0.5" Width="11"/>
        </Canvas>
    </Viewbox>

