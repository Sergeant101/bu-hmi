            </Canvas>
            <Canvas Name="Switch_close" Height="40" Width="40" d:IsLocked="True" d:IsHidden="True">
                <Path StrokeThickness="2" Stroke="#ff000000" StrokeMiterLimit="1.0" Data="F1 M 113,0 L 130,0" Canvas.Left="11" Stretch="Fill" Width="19" Canvas.Top="6"/>
