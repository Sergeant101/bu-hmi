        <Style x:Key="ngo_scada_button_style" TargetType="{x:Type se:ScadaButton}">
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="FontSize" Value="14" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type se:ScadaButton}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="12"/>
                                <RowDefinition Height="1*"/>
                                <RowDefinition Height="13"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="12"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="12"/>
                            </Grid.ColumnDefinitions>
                            <Image x:Name="l_top"      Grid.Row="0" Grid.Column="0" Source="{StaticResource left-top-normal}"    HorizontalAlignment="Stretch" Stretch="Uniform" SnapsToDevicePixels="True"/>
                            <Image x:Name="l_bg"       Grid.Row="1" Grid.Column="0" Source="{StaticResource left-bg-normal}"     HorizontalAlignment="Stretch" Stretch="Fill"    SnapsToDevicePixels="True" Margin="0,-1,0,-1"/>
                            <Image x:Name="l_bottom"   Grid.Row="2" Grid.Column="0" Source="{StaticResource left-bottom-normal}" HorizontalAlignment="Stretch" Stretch="Uniform" SnapsToDevicePixels="True"/>

                            <Image x:Name="top"        Grid.Row="0" Grid.Column="1" Source="{StaticResource top-normal}"    HorizontalAlignment="Stretch"   Stretch="Fill"  SnapsToDevicePixels="True"  Margin="-1,0,-1,0"/>
                            <Grid  Grid.Row="1" Grid.Column="1"                                      Margin="-1,-1,-1,-1">
                                <Image x:Name="center"   Source="{StaticResource center-normal}"    HorizontalAlignment="Stretch" Stretch="Fill"   SnapsToDevicePixels="True"   Margin="-1,0,-1,0"/>
                                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
        							Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" RecognizesAccessKey="True" />
                            </Grid>
                            <Image x:Name="bottom"     Grid.Row="2" Grid.Column="1" Source="{StaticResource bottom-normal}"  HorizontalAlignment="Stretch"  SnapsToDevicePixels="True"  Stretch="Fill"  Margin="0,0,0,0"/>


                            <Image x:Name="r_top"      Grid.Row="0" Grid.Column="2" Source="{StaticResource right-top-normal}"    HorizontalAlignment="Stretch" SnapsToDevicePixels="True" Stretch="None" />
                            <Image x:Name="r_bg"       Grid.Row="1" Grid.Column="2" Source="{StaticResource right-bg-normal}"     HorizontalAlignment="Stretch" SnapsToDevicePixels="True" Stretch="Fill" Margin="0,-1,0,-1"/>
                            <Image x:Name="r_bottom"   Grid.Row="2" Grid.Column="2" Source="{StaticResource right-bottom-normal}" HorizontalAlignment="Stretch" SnapsToDevicePixels="True" Stretch="None"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter TargetName="l_top"      Property="Source" Value="{StaticResource left-top-push}"/>
                                <Setter TargetName="l_bg"       Property="Source" Value="{StaticResource left-bg-push}"     />
                                <Setter TargetName="l_bottom"   Property="Source" Value="{StaticResource left-bottom-push}" />

                                <Setter TargetName="top"        Property="Source" Value="{StaticResource top-push}"         />
                                <Setter TargetName="center"     Property="Source" Value="{StaticResource center-push}"      />
                                <Setter TargetName="bottom"     Property="Source" Value="{StaticResource bottom-push}"      />


                                <Setter TargetName="r_top"      Property="Source" Value="{StaticResource right-top-push}"   />
                                <Setter TargetName="r_bg"       Property="Source" Value="{StaticResource right-bg-push}"    />
                                <Setter TargetName="r_bottom"   Property="Source" Value="{StaticResource right-bottom-push}"/>
                            </Trigger>

                            <Trigger Property="IsSelected" Value="True">
                                <Setter TargetName="l_top"      Property="Source" Value="{StaticResource left-top-selected}"/>
                                <Setter TargetName="l_bg"       Property="Source" Value="{StaticResource left-bg-selected}"     />
                                <Setter TargetName="l_bottom"   Property="Source" Value="{StaticResource left-bottom-selected}" />

                                <Setter TargetName="top"        Property="Source" Value="{StaticResource top-selected}"         />
                                <Setter TargetName="center"     Property="Source" Value="{StaticResource center-selected}"      />
                                <Setter TargetName="bottom"     Property="Source" Value="{StaticResource bottom-selected}"      />


                                <Setter TargetName="r_top"      Property="Source" Value="{StaticResource right-top-selected}"   />
                                <Setter TargetName="r_bg"       Property="Source" Value="{StaticResource right-bg-selected}"    />
                                <Setter TargetName="r_bottom"   Property="Source" Value="{StaticResource right-bottom-selected}"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="Foreground" Value="#ADADAD"/>
                                <!--<Setter Property="Background" Value="{StaticResource img_scada_button_disable_small}"/>-->

                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

