            <DrawingBrush x:Key="CrossLine" AlignmentX="Left" AlignmentY="Top" Stretch="Uniform">
                <DrawingBrush.Drawing>
                    <GeometryDrawing>
                        <GeometryDrawing.Pen>
                            <Pen Brush="Black"  Thickness="2" EndLineCap="Square" StartLineCap="Square"/>
                        </GeometryDrawing.Pen>
                        <GeometryDrawing.Geometry>
                            <PathGeometry>
                                <PathFigure IsClosed="True" StartPoint="0,0">
                                    <LineSegment Point="20,20" IsStroked="False"/>
                                </PathFigure>
                                <PathFigure IsClosed="True" StartPoint="20,0">
                                    <LineSegment Point="0,20" IsStroked="False" />
                                </PathFigure>
                            </PathGeometry>
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                </DrawingBrush.Drawing>
            </DrawingBrush>

            <VisualBrush x:Key="CrossLineBrush" TileMode="Tile" Viewport="0,0,8,8" ViewportUnits="Absolute">
                <VisualBrush.Visual>
                    <Rectangle Width="1" Height="1" Fill="{StaticResource CrossLine}" SnapsToDevicePixels="True"/>
                </VisualBrush.Visual>
            </VisualBrush>

            <DrawingBrush x:Key="Ellipse" AlignmentX="Left" AlignmentY="Top" Stretch="Uniform">
