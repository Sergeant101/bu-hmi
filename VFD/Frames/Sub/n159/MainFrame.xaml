                        </GeometryDrawing.Pen>
                        <GeometryDrawing.Geometry>
                            <EllipseGeometry RadiusX="10" RadiusY="10" />
                        </GeometryDrawing.Geometry>
                    </GeometryDrawing>
                </DrawingBrush.Drawing>
            </DrawingBrush>

            <VisualBrush x:Key="EllipseBrush" TileMode="Tile" Viewport="0,0,10,10" ViewportUnits="Absolute">
                <VisualBrush.Visual>
                    <Rectangle Width="2" Height="2" Fill="{StaticResource Ellipse}" SnapsToDevicePixels="True"/>
                </VisualBrush.Visual>
            </VisualBrush>

