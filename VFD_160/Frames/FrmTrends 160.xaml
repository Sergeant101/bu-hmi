                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </se:TrendViewer.LayoutTransform>
                    <se:TrendViewer.TrendNodes>
                        <Custom:HmiNodeDescriptor Comment="pump1" ItemType="Trend" Path="pump1"/>
