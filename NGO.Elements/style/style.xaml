        <Setter Property="BorderBrush">
            <Setter.Value>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFCFCFCF" Offset="0"/>
                    <GradientStop Color="#FFC3C3C3" Offset="1"/>
                    <GradientStop Color="#FF9E9E9E" Offset="0.243"/>
                    <GradientStop Color="#FFA0A0A0" Offset="0.778"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
