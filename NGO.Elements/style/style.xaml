            <Setter.Value>
                <ControlTemplate>
                    <Viewbox
                                    SnapsToDevicePixels="True"
                                    Width="{Binding ActualWidth,RelativeSource={RelativeSource AncestorType=se:VisualBox}}"
                                    Height="{Binding ActualHeight,RelativeSource={RelativeSource AncestorType=se:VisualBox}}">
                        <Grid
                                    Width="50"
                                    Height="50">

