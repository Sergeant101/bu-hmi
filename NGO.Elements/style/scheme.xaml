            <Setter.Value>
                <ControlTemplate>
                    <Viewbox>
                        <Border Width="80" Height="90" BorderBrush="Black" BorderThickness="2">
                            <Border.Background>
                                <PriorityBinding>
                                    <Binding RelativeSource="{RelativeSource AncestorType=se:VisualBox}" Path="VisualStateCollection.CurrentState.Background" />
                                    <Binding RelativeSource="{RelativeSource AncestorType=se:VisualBox}" Path="Background" />
                                </PriorityBinding>
                            </Border.Background>
                            <Grid>
                                <Border Width="2" Height="10" Grid.Row="0" Background="Black" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                                <Border Width="30" Height="70" Grid.Row="1" BorderBrush="Black" BorderThickness="2" VerticalAlignment="Center"/>
                                <Border Width="2" Height="10" Grid.Row="2" Background="Black" HorizontalAlignment="Center" VerticalAlignment="Top"/>
                            </Grid>
                        </Border>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

