    <Style TargetType="se:VisualBox" x:Key="Motor">
        <Setter Property="Background" Value="Gold"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Viewbox SnapsToDevicePixels="True">
                        <Grid Width="40" Height="40" SnapsToDevicePixels="True">
                            <Ellipse HorizontalAlignment="Stretch" SnapsToDevicePixels="True" VerticalAlignment="Stretch" Margin="0" Stroke="Black" StrokeThickness="1" >
                                <Ellipse.Fill>
                                    <PriorityBinding>
                                        <Binding RelativeSource="{RelativeSource AncestorType=se:VisualBox}" Path="VisualStateCollection.CurrentState.Background" />
                                        <Binding RelativeSource="{RelativeSource AncestorType=se:VisualBox}" Path="Background" />
                                    </PriorityBinding>
                                </Ellipse.Fill>
                            </Ellipse>
                            <Ellipse HorizontalAlignment="Stretch" SnapsToDevicePixels="True" VerticalAlignment="Stretch" Margin="8" Stroke="Black" StrokeThickness="1"/>
                        </Grid>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

